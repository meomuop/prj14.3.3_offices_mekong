<script>
$(function() {
	$( "#div_kdl" ).draggable();
	$( "#add_edit_kdl" ).draggable();
	$( "#add_edit_dl" ).draggable();
	$( "#dl_file" ).draggable();
});
function min_size_kdl(){
	if(document.getElementById('div_kdl').style.height=='550px'){
		document.getElementById('div_kdl').style.height='20px';
		document.getElementById('kdl_main_cont').style.height='20px';
		document.getElementById('kdl_content_outsign').style.display='none';
		document.getElementById('kdl_content_insign').style.display='none';
		document.getElementById('kdl_tool_bar').style.display='none';
		document.getElementById('kdl_menu').style.display='none';
	}else{
		document.getElementById('div_kdl').style.height='550px';
		document.getElementById('kdl_main_cont').style.height='550px';
		document.getElementById('kdl_content_outsign').style.display='';
		document.getElementById('kdl_content_insign').style.display='';
		document.getElementById('kdl_tool_bar').style.display='';
		document.getElementById('kdl_menu').style.display='';
	}
}
function reset_pos_kdl(){
	document.getElementById('div_kdl').style.margin='40px 200px';
}
function max_size_kdl(){
	if(document.getElementById('div_kdl').style.width=='50%'){
		document.getElementById('div_kdl').style.margin='0';
		document.getElementById('div_kdl').style.width='98%';
		document.getElementById('div_kdl').style.height='600px';
		document.getElementById('kdl_main_cont').style.height='600px';
		document.getElementById('kdl_content_outsign').style.height='545px';
		document.getElementById('kdl_content_insign').style.height='523px';
	}else{
		document.getElementById('div_kdl').style.margin='10px 200px';
		document.getElementById('div_kdl').style.width='50%';
		document.getElementById('div_kdl').style.height='550px';
		document.getElementById('kdl_main_cont').style.height='550px';
		document.getElementById('kdl_content_outsign').style.height='495px';
		document.getElementById('kdl_content_insign').style.height='473px';
	}
}
function change_style_kdl(obj){
	for(i=1;i<=3;i++){
		if(i==obj){
			document.getElementById('c_menu_kdl_'+i).style.borderLeft = '1px solid #99bbe8';
			document.getElementById('c_menu_kdl_'+i).style.borderRight = '1px solid #99bbe8';
			document.getElementById('c_menu_kdl_'+i).style.borderBottom = '1px solid #dfe8f6';
			document.getElementById('c_menu_kdl_'+i).style.backgroundColor = '#dfe8f6';
			document.getElementById('c_menu_kdl_'+i).style.height = '25px';
			document.getElementById('c_menu_kdl_'+i).style.marginTop = '0px';
		}
		else{
			if(i!=1){
				document.getElementById('c_menu_kdl_'+i).style.borderLeft = 'none';
			}
			if(i==obj-1){
				document.getElementById('c_menu_kdl_'+i).style.borderRight = 'none';
			}else{
				document.getElementById('c_menu_kdl_'+i).style.borderRight = '1px solid #99bbe8';
			}
			document.getElementById('c_menu_kdl_'+i).style.borderBottom = 'none';
			document.getElementById('c_menu_kdl_'+i).style.backgroundColor = '#ccd8e7';
			document.getElementById('c_menu_kdl_'+i).style.height = '20px';
			document.getElementById('c_menu_kdl_'+i).style.marginTop = '4px';
		}
	}
}
</script>
