function min_size_hdban(){
	if(document.getElementById('div_hdban').style.height=='550px'){
		document.getElementById('div_hdban').style.height='20px';
		document.getElementById('hdban_main_cont').style.height='20px';
		document.getElementById('hdban_content_outsign').style.display='none';
		document.getElementById('hdban_content_insign').style.display='none';
		document.getElementById('hdban_tool_bar').style.display='none';
		document.getElementById('hdban_menu').style.display='none';
	}else{
		document.getElementById('div_hdban').style.height='550px';
		document.getElementById('hdban_main_cont').style.height='550px';
		document.getElementById('hdban_content_outsign').style.display='';
		document.getElementById('hdban_content_insign').style.display='';
		document.getElementById('hdban_tool_bar').style.display='';
		document.getElementById('hdban_menu').style.display='';
	}
}
function reset_pos_hdban(){
	document.getElementById('div_hdban').style.margin='40px 200px';
}
function max_size_hdban(){
	if(document.getElementById('div_hdban').style.width=='70%'){
		document.getElementById('div_hdban').style.margin='0';
		document.getElementById('div_hdban').style.width='98%';
		document.getElementById('div_hdban').style.height='600px';
		document.getElementById('hdban_main_cont').style.height='600px';
		document.getElementById('hdban_content_outsign').style.height='545px';
		document.getElementById('hdban_content_insign').style.height='523px';
	}else{
		document.getElementById('div_hdban').style.margin='10px 200px';
		document.getElementById('div_hdban').style.width='70%';
		document.getElementById('div_hdban').style.height='550px';
		document.getElementById('hdban_main_cont').style.height='550px';
		document.getElementById('hdban_content_outsign').style.height='495px';
		document.getElementById('hdban_content_insign').style.height='473px';
	}
}

function change_style_hdban(obj){

	for(i=1;i<=6;i++){
		if(i==parseInt(obj)){
            if(document.getElementById('c_menu_hdban_'+i) === null) {
                continue;
            }else {
                document.getElementById('c_menu_hdban_' + i).style.borderLeft = '1px solid #99bbe8';
                document.getElementById('c_menu_hdban_' + i).style.borderRight = '1px solid #99bbe8';
                document.getElementById('c_menu_hdban_' + i).style.borderBottom = '1px solid #dfe8f6';
                document.getElementById('c_menu_hdban_' + i).style.backgroundColor = '#dfe8f6';
                document.getElementById('c_menu_hdban_' + i).style.height = '25px';
                document.getElementById('c_menu_hdban_' + i).style.marginTop = '0px';
            }

            if(parseInt(obj)==6){
                document.getElementById('c_menu_hdban_5').style.borderRight = '1px solid #99bbe8';
            }
		}
		else{
            if(document.getElementById('c_menu_hdban_'+i) === null) {
                continue;
            }else {
                if (i != 1) {
                    document.getElementById('c_menu_hdban_' + i).style.borderLeft = 'none';
                }
                if (i == parseInt(obj) - 1) {
                    document.getElementById('c_menu_hdban_' + i).style.borderRight = 'none';
                } else {
                    document.getElementById('c_menu_hdban_' + i).style.borderRight = '1px solid #99bbe8';
                }
                document.getElementById('c_menu_hdban_' + i).style.borderBottom = 'none';
                document.getElementById('c_menu_hdban_' + i).style.backgroundColor = '#ccd8e7';
                document.getElementById('c_menu_hdban_' + i).style.height = '20px';
                document.getElementById('c_menu_hdban_' + i).style.marginTop = '4px';
            }

            if(parseInt(obj)!=6){
                document.getElementById('c_menu_hdban_6').style.borderLeft = '1px solid #99bbe8';
            }
		}
	}
}
</script>
