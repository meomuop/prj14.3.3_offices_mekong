<script language="javascript">	
	var div_arr_hdban = ['list_file_hdban','div_view_hdban','div_thuchien_hdban','div_hdban_lkvb','list_file_dthdban'];

	function change_bgcl_hdban(obj){
		for(i=0;i<=div_arr_hdban.length;i++){
			if(div_arr_hdban[i] == obj){
				document.getElementById('div_hdban').style.zIndex='2000';
				document.getElementById(obj).style.zIndex = '2001'; 
				document.getElementById(obj).style.borderColor='#F00';
			}else{
				var element =  document.getElementById(div_arr_hdban[i]);
				if (typeof(element) != 'undefined' && element != null)
				{
					element.style.zIndex='2000';
					element.style.borderColor='#99bbe8';
				}
			}
		}
	}
	
	
	$("#hdban_content_outsign").mouseover( function () { 
		$( "#div_hdban" ).draggable({ disabled: true });
	});
	$("#hdban_content_outsign").mouseout( function () { 
		$( "#div_hdban" ).draggable({ disabled: false });
	});
	
	$("#hdban_menu").mouseover( function () { 
		$( "#div_hdban" ).draggable({ disabled: true });
	});
	$("#hdban_menu").mouseout( function () { 
		$( "#div_hdban" ).draggable({ disabled: false });
