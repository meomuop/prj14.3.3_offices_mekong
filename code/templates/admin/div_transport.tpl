	$( "#tnvb_report" ).draggable();
	//$( "#div_view_transport" ).draggable();
});
function min_size_transport(){
	if(document.getElementById('div_transport').style.height=='550px'){
		document.getElementById('div_transport').style.height='20px';
		document.getElementById('transport_main_cont').style.height='20px';
		document.getElementById('transport_content_outsign').style.display='none';
		document.getElementById('transport_content_insign').style.display='none';
		document.getElementById('transport_tool_bar').style.display='none';
		document.getElementById('transport_menu').style.display='none';
	}else{
		document.getElementById('div_transport').style.height='550px';
		document.getElementById('transport_main_cont').style.height='550px';
		document.getElementById('transport_content_outsign').style.display='';
		document.getElementById('transport_content_insign').style.display='';
		document.getElementById('transport_tool_bar').style.display='';
		document.getElementById('transport_menu').style.display='';
	}
}
function reset_pos_transport(){
	document.getElementById('div_transport').style.margin='40px 200px';
}
function max_size_transport(){
	if(document.getElementById('div_transport').style.width=='70%'){
		document.getElementById('div_transport').style.margin='0';
		document.getElementById('div_transport').style.width='98%';
		document.getElementById('div_transport').style.height='600px';
		document.getElementById('transport_main_cont').style.height='600px';
		document.getElementById('transport_content_outsign').style.height='545px';
		document.getElementById('transport_content_insign').style.height='523px';
	}else{
		document.getElementById('div_transport').style.margin='10px 200px';
		document.getElementById('div_transport').style.width='70%';
		document.getElementById('div_transport').style.height='550px';
		document.getElementById('transport_main_cont').style.height='550px';
		document.getElementById('transport_content_outsign').style.height='495px';
		document.getElementById('transport_content_insign').style.height='473px';
	}
}

function change_style_transport(obj){
	for(i=1;i<=9;i++){
		if(i==obj){
			document.getElementById('c_menu_transport_'+i).style.borderLeft = '1px solid #99bbe8';
			document.getElementById('c_menu_transport_'+i).style.borderRight = '1px solid #99bbe8';
			document.getElementById('c_menu_transport_'+i).style.borderBottom = '1px solid #dfe8f6';
			document.getElementById('c_menu_transport_'+i).style.backgroundColor = '#dfe8f6';
			document.getElementById('c_menu_transport_'+i).style.height = '25px';
			document.getElementById('c_menu_transport_'+i).style.marginTop = '0px';
		}
		else{
			if(i!=1){
				document.getElementById('c_menu_transport_'+i).style.borderLeft = 'none';
			}
			if(i==obj-1){
				document.getElementById('c_menu_transport_'+i).style.borderRight = 'none';
			}else{
				document.getElementById('c_menu_transport_'+i).style.borderRight = '1px solid #99bbe8';
			}
			document.getElementById('c_menu_transport_'+i).style.borderBottom = 'none';
			document.getElementById('c_menu_transport_'+i).style.backgroundColor = '#ccd8e7';
			document.getElementById('c_menu_transport_'+i).style.height = '20px';
			document.getElementById('c_menu_transport_'+i).style.marginTop = '4px';
		}
	}
}
</script>
